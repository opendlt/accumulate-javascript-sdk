{
  "name": "accumulate.js",
  "version": "0.8.7",
  "description": "Accumulate JS/TS SDK",
  "keywords": [
    "blockchain",
    "accumulate"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Accumulate Foundation",
  "license": "MIT",
  "homepage": "https://gitlab.com/accumulatenetwork/sdk/javascript",
  "bugs": "https://gitlab.com/accumulatenetwork/sdk/javascript/-/issues",
  "repository": "gitlab:accumulatenetwork/sdk/javascript",
  "private": false,
  "files": [
    "/lib",
    "/src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test:unit": "jest --config=test/jest.config.js",
    "test:integration": "jest --config=test-it/jest.config.js",
    "test:all": "jest",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --config .prettierrc 'src/**/*.ts' 'examples/**/*.ts' 'test/**/*.ts' 'test-it/**/*.ts' --write",
    "format:check": "prettier --config .prettierrc 'src/**/*.ts' 'examples/**/*.ts' 'test/**/*.ts' 'test-it/**/*.ts' --check",
    "clean": "rm -rf lib build coverage docs",
    "ts-node": "ts-node",
    "doc": "typedoc --treatWarningsAsErrors",
    "prepublish": "yarn code-check",
    "code-check": "yarn clean && yarn build && yarn format:check && yarn lint && yarn doc"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^18.7.18",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "tree-kill": "^1.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.15",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "multibase": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "tweetnacl": "^1.0.3",
    "@ledgerhq/devices": "^8.0.4",
    "@ledgerhq/hw-transport": "^6.28.5",
    "@ledgerhq/hw-transport-node-hid": "^6.27.17",
    "@ledgerhq/hw-transport-u2f": "^5.36.0-deprecated",
    "@ledgerhq/logs": "^6.10.1",
    "@ledgerhq/react-native-hid": "^6.29.4",
    "@types/node-hid": "^1.3.1",
    "react-native": "^0.58.6"
  },
  "exports": {
    ".": "./lib/index.js",
    "./address": "./lib/address/index.js",
    "./api_v2": "./lib/api_v2/index.js",
    "./api_v3": "./lib/api_v3/index.js",
    "./common": "./lib/common/index.js",
    "./core": "./lib/core/index.js",
    "./encoding": "./lib/encoding/index.js",
    "./errors": "./lib/errors/index.js",
    "./merkle": "./lib/merkle/index.js",
    "./messaging": "./lib/messaging/index.js",
    "./network": "./lib/network/index.js",
    "./signing": "./lib/signing/index.js",
    "./lib/address": "./lib/address/index.js",
    "./lib/api_v2": "./lib/api_v2/index.js",
    "./lib/api_v3": "./lib/api_v3/index.js",
    "./lib/common": "./lib/common/index.js",
    "./lib/core": "./lib/core/index.js",
    "./lib/encoding": "./lib/encoding/index.js",
    "./lib/errors": "./lib/errors/index.js",
    "./lib/merkle": "./lib/merkle/index.js",
    "./lib/messaging": "./lib/messaging/index.js",
    "./lib/network": "./lib/network/index.js",
    "./lib/signing": "./lib/signing/index.js"
  },
  "peerDependencies": {
    "@ledgerhq/devices": "^8.0.4",
    "@ledgerhq/errors": "^6.12.7",
    "@ledgerhq/hw-transport": "^6.28.5",
    "@ledgerhq/hw-transport-node-hid": "^6.27.17",
    "@ledgerhq/hw-transport-node-hid-noevents": "^6.27.16",
    "@ledgerhq/hw-transport-mocker": "^6.27.16",
    "@ledgerhq/logs": "^6.10.1",
    "lodash": "^4.17.21",
    "usb": "^1.7.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  }
}
