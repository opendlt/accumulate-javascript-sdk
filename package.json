{
  "name": "accumulate.js",
  "version": "0.12.0",
  "description": "Accumulate JS/TS SDK",
  "keywords": [
    "blockchain",
    "accumulate"
  ],
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Accumulate Foundation",
  "license": "MIT",
  "homepage": "https://gitlab.com/accumulatenetwork/sdk/javascript",
  "bugs": "https://gitlab.com/accumulatenetwork/sdk/javascript/-/issues",
  "repository": "gitlab:accumulatenetwork/sdk/javascript",
  "private": false,
  "files": [
    "/lib",
    "/src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:browser": "webpack --config webpack.browser.config.js && webpack --config webpack.browser-test.config.js",
    "test:unit": "jest --config=test/jest.config.cjs",
    "test:integration": "jest --config=test-it/jest.config.js",
    "test:browser": "jest --config=test/jest.browser.config.js",
    "test:browser:serve": "npx http-server -o /test/browser-test.html",
    "test:all": "jest --testPathIgnorePatterns=browser-test",
    "test:update": "ts-jest config:migrate test-it/jest.config.js",
    "lint": "eslint src/ test/ --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --config .prettierrc 'src/**/*.ts' 'examples/**/*.ts' 'test/**/*.ts' 'test-it/**/*.ts' --write",
    "format:check": "prettier --config .prettierrc 'src/**/*.ts' 'examples/**/*.ts' 'test/**/*.ts' 'test-it/**/*.ts' --check",
    "clean": "rm -rf lib build coverage docs node_modules",
    "ts-node": "ts-node",
    "doc": "typedoc --treatWarningsAsErrors --plugin typedoc-plugin-missing-exports --excludeExternals",
    "prepublish": "yarn code-check",
    "code-check": "yarn lint && prettier --check 'src/**/*.{js,jsx,ts,tsx}' 'test/**/*.{js,jsx,ts,tsx}'",
    "prepare": "yarn build"
  },
